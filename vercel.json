{
  "devCommand": "uvicorn app.main:app --host 0.0.0.0 --port 3000",
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api/index.py" }
  ],
  "functions": {
    "api/**/*.py": { "runtime": "python3.11" }
  },
  "env": {
    "ENABLE_LINTER": "false"
  }
}
